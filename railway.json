{
    "build": {
      "builder": "DOCKERFILE",
      "dockerfilePath": "Dockerfile.railway"
    },
    "deploy": {
      "startCommand": "docker-compose up",
      "healthcheckPath": "/",
      "healthcheckTimeout": 100,
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }